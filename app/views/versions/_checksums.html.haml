- unless @checksum_groups[tcv].nil? || @checksum_groups[tcv].empty?
  %dl.row.mb-0
    - @checksum_groups[tcv].keys.each do |checksum|
      %dt.col-3.mb-0.text-right.text-monospace= '✔︎' + checksum[-5,5] + ': '
      %dd.col-9.mb-0= @checksum_groups[tcv][checksum].map { |comp| comp.name }.sort_by { |comp_name| comp_name.downcase }.join(', ')
